{
  "name": "widgets-microfrontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/widgets-microfrontend/src",
  "implicitDependencies": ["shared-ui", "shared-utils"],
  "targets": {
    "serve": {
      "executor": "@nx/next:server",
      "dependsOn": ["^build"],
      "options": {
        "buildTarget": "widgets-microfrontend:build",
  "dev": true,
  "port": 5002
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "dependsOn": ["^build"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/widgets-microfrontend"
      }
    }
  },
  "tags": []
}