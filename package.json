{
  "name": "@all-in-one/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "serve:auth-microfrontend": "nx serve auth-microfrontend",
    "serve:widgets-microfrontend": "nx serve widgets-microfrontend",
    "serve:dashboard-shell": "nx serve dashboard-shell",
  "serve:auth-service": "nx serve auth-service",
  "serve:dashboard-service": "nx serve dashboard-service",
    "serve:data-source-service": "nx serve @all-in-one/data-source-service",
    "serve:all-frontend": "nx run-many --target=serve --projects=auth-microfrontend,widgets-microfrontend,dashboard-shell --parallel=3",
  "serve:all-backend": "nx run-many --target=serve --projects=auth-service,dashboard-service,data-source-service --parallel=3",
  "serve:all": "nx run-many --target=serve --projects=auth-microfrontend,widgets-microfrontend,dashboard-shell,auth-service,dashboard-service,data-source-service --parallel=6",
    "fix-tsconfig": "node scripts/fix-tsconfig.js",
    "serve:all-fixed": "npm run serve:all & timeout /t 10 & npm run fix-tsconfig",
    "build:libs": "nx run-many --target=build --projects=shared-ui,shared-utils",
    "build:all-libs": "nx run-many --target=build --projects=shared-ui,shared-utils --parallel=2",
    "typecheck": "tsc --noEmit --project tsconfig.base.json",
    "typecheck:apps": "npm run typecheck:auth && npm run typecheck:widgets && npm run typecheck:dashboard",
    "typecheck:auth": "tsc --noEmit --project apps/auth-microfrontend/tsconfig.app.json",
    "typecheck:widgets": "tsc --noEmit --project apps/widgets-microfrontend/tsconfig.app.json",
    "typecheck:dashboard": "tsc --noEmit --project apps/dashboard-shell/tsconfig.app.json",
    "lint:strict": "eslint . --max-warnings 0",
    "lint:apps": "eslint apps/*/src --max-warnings 5",
    "lint:libs": "eslint libs/*/src --max-warnings 0",
    "build:all": "nx run-many --target=build --all",
    "test:all": "nx run-many --target=test --all",
    "lint:all": "nx run-many --target=lint --all"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/schematics": "^19.2.15",
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@next/eslint-plugin-next": "^15.2.4",
    "@nx/eslint": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/jest": "21.3.11",
    "@nx/js": "21.3.11",
    "@nx/nest": "^21.3.11",
    "@nx/next": "^21.3.11",
    "@nx/node": "21.3.11",
    "@nx/react": "^21.3.11",
    "@nx/vite": "21.3.11",
    "@nx/web": "^21.3.11",
    "@nx/webpack": "21.3.11",
    "@svgr/webpack": "^8.1.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.38",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^3.0.0",
    "ajv": "^8.0.0",
    "autoprefixer": "10.4.13",
    "babel-jest": "^30.0.2",
    "eslint": "^9.8.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.3.11",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "^3.0.0",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "axios": "^1.6.0",
    "next": "~15.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  }
}
